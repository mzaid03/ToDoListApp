{
  "name": "todo-fullstack",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.17 <21"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "prisma migrate deploy || prisma db push && next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
