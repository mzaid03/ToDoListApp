    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "24.4.0",
    "@types/react": "19.1.13",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.49",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.10",
    "typescript": "5.5.4"
  }
}
